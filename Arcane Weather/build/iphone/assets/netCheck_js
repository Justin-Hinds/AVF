var networkCheck=function(e){var o="http://api.wunderground.com/api/a533683612fcf770/forecast/geolookup/conditions/q/"+e.lat+","+e.lon+".json";if(Ti.Network.online){var r=Ti.Network.createHTTPClient();r.onload=function(){var e=JSON.parse(this.responseText),o={forecast:e.forecast,location:e.location,current:e.current_observation};ui.bUi(o),console.log(o.current)},r.open("GET",o),r.send()}else alert("Network currently unavailable. Please check settings.")};exports.networkCheck=networkCheck;