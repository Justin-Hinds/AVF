Ti.UI.setBackgroundColor("fff");var win=Ti.UI.createWindow({layout:"vertical"}),fFont,cFont,bUi=function(e){var t=Ti.UI.createLabel({text:e.city+", "+e.state,top:10,left:15,font:{fontSize:34,fontFamily:"Futura-Medium"}}),o=Ti.UI.createLabel({text:"Winds: "+e.currentWinds+"mph",top:10,right:180,font:{fontSize:24,fontFamily:"Futura-Medium"}}),a=Ti.UI.createLabel({text:"Precip: "+e.currentPrecip+"%",top:10,right:15,font:{fontSize:24,fontFamily:"Futura-Medium"}}),r=Ti.UI.createLabel({text:e.currentTemp+"°",top:70,left:50,font:{fontSize:60,fontFamily:"Futura-Medium"}}),n=Ti.UI.createLabel({text:e.currentWeath,bottom:20,left:45,font:{fontSize:34,fontFamily:"Futura-Medium"}}),c=Ti.UI.createView({backgroundColor:"aed6f0",height:"20%",top:20,width:"80%",viewShadowColor:"373737",viewShadowRadius:6,viewShadowOffset:-10,layout:"horizontal"}),f=Ti.UI.createView({backgroundColor:"aed6f0",height:"20%",top:20,width:"80%",viewShadowColor:"373737",viewShadowRadius:6,viewShadowOffset:-10}),l=Ti.UI.createImageView({backgroundColor:"white",image:e.currentIcon,height:80,opacity:.5,top:70,alighn:"center",width:80,viewShadowColor:"373737",viewShadowRadius:6,viewShadowOffset:-10});for(f.add(t,o,a,r,l,n),i=0;5>i;i++){var d=Ti.UI.createLabel({text:e.forecast.tenDayCast[i].day,align:"center",top:10,left:15}),u=Ti.UI.createLabel({text:"High: "+e.forecast.tenDayCast[i].hi+"°",top:5,left:15,font:{fontSize:20,fontStyle:"Optima-ExtraBlack"}}),s=Ti.UI.createLabel({text:"Low: "+e.forecast.tenDayCast[i].lo+"°",top:0,left:15,font:{fontSize:20,fontStyle:"Optima-ExtraBlack"}}),h=Ti.UI.createImageView({backgroundColor:"white",image:e.forecast.tenDayCast[i].icon,height:60,width:60,top:10,viewShadowColor:"373737",viewShadowRadius:6,viewShadowOffset:-10}),w=Ti.UI.createView({backgroundColor:"white",height:"15%",top:0,left:"4%",width:"15%",opacity:.5,viewShadowColor:"373737",viewShadowRadius:6,viewShadowOffset:-10,layout:"vertical"});w.add(d,h,u,s),c.add(w)}win.add(f,c)};win.open(),exports.bUi=bUi;